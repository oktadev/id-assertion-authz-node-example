{
  "name": "id-assertion-authz-node-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx prisma generate --schema=todo0/prisma/schema.prisma && npx prisma generate --schema=wiki0/prisma/schema.prisma",
    "server:install": "cd ./authorization-server && yarn install && cd ../",
    "preinstall": "yarn build:request && yarn link request-jwt-auth-grant",
    "build:request": "cd request-jwt-auth-grant && yarn link && yarn install && yarn build && cd ..",
    "dev:todo": "cd todo0 && nodemon -e ts -w src/server -x 'tsx src/server/main.ts'",
    "dev:wiki": "cd wiki0 && nodemon -e ts -w src/server -x 'tsx src/server/main.ts'",
    "auth:todo": "cd ./authorization-server && yarn server:todo",
    "auth:wiki": "cd ./authorization-server && yarn server:wiki",
    "start:todo0": "cd todo0 && NODE_ENV=production tsx src/server/main.ts",
    "start:wiki0": "cd wiki0 && NODE_ENV=production tsx src/server/main.ts",
    "build:todo": "cd ./todo0/ && vite build",
    "build:wiki": "cd ./wiki0/ && vite build",
    "resetdb": "cd todo0 && npx prisma migrate reset --schema=prisma/schema.prisma && cd ../wiki0 && npx prisma migrate reset --schema=prisma/schema.prisma && cd .. && npx tsx testdata/reset_data.ts",
    "migratedb": "cd todo0 && npx prisma migrate dev --schema prisma/schema.prisma && cd - && cd wiki0 && npx prisma migrate dev --schema prisma/schema.prisma",
    "lint": "npx --yes eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "npx --yes eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.8.1",
