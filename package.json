    "@tiptap/extension-text-style": "^2.2.1",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/react": "^2.2.1",
    "@tiptap/starter-kit": "^2.2.1",
    "connect-redis": "^7.1.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "find-config": "^1.0.0",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "jwks-rsa": "^3.1.0",
