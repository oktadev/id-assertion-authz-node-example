{
  "name": "mcp-bedrock-client",
  "version": "1.0.0",
  "description": "MCP Client with AWS Bedrock LLM integration",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:web": "node dist/index.js",
    "start:todo-server": "node dist/mcp-server/todo-server.js",
    "dev": "nodemon -e ts -w src -x 'tsx src/index.ts'",
    "dev:web": "yarn build && yarn start:web",
    "dev:todo-server": "yarn build && yarn start:todo-server",
    "examples": "npm run build && node dist/examples.js",
    "clean": "rm -rf dist",
    "watch": "tsc --watch",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "aws",
    "bedrock",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/find-config": "^1.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-openidconnect": "^0.1.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.844.0",
    "@aws-sdk/credential-providers": "^3.846.0",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@prisma/client": "^6.12.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-session": "^1.18.0",
    "find-config": "^1.0.0",
    "jwks-rsa": "^3.1.0",
    "morgan": "^1.10.0",
    "passport": "0.5.0",
    "passport-jwt": "^4.0.1",
    "passport-openidconnect": "^0.1.2",
    "prisma": "^6.11.1",
    "react": "^18.3.0",
    "redis": "^4.6.13",
    "socket.io": "^4.8.1"
  }
}
